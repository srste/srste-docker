{
  "name": "MetaDataSource",
  "plural": "MetaDataSources",
  "idInjection": false,
  "base": "PersistedModel",
  "options": {
    "validateUpsert": true,
    "public":true
  },
  "properties": {
    "uid": {
      "type": "String",
      "id": true,
      "defaultFn": "guid"
    },
    "Name": {
      "type": "String",
      "required": true
    },
    "Scope": {
      "type": "String",
      "required": true
    },
    "Connection": {
      "type": "Text"      
    },
    "Type": {
      "type": "String"
    },
    "srs_created_at": {
      "type": "date",
      "dataType": "timestamp",
      "defaultFn": "now"
    },  
    "srs_updated_at": {
      "type": "date",
      "dataType": "timestamp",
      "defaultFn": "now"
    },
    "srs_created_by": {
      "type": "String",
      "default": "System"
    },
    "srs_updated_by": {
      "type": "String",
      "default": "System"
    }
  }
}
